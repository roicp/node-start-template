{
  "name": "node-start-template",
  "version": "0.0.1",
  "description": "A basic NodeJs and Express start template",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prestart": "babel ./server/src --out-dir ./server/build",
    "start": "node ./server/build/bin/www.js",
    "startdev": "nodemon --exec babel-node ./server/src/bin/www.js",
    "test": "jest --coverage",
    "lint": "./node_modules/.bin/eslint ./server/src",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roicp/node-start-template.git"
  },
  "keywords": [
    "NodeJs",
    "ExpressJs",
    "Javascript"
  ],
  "author": "Rodrigo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roicp/node-start-template/issues"
  },
  "homepage": "https://github.com/roicp/node-start-template#readme",
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
